{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Simple React boilerplate.",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "webpack-dev-server  --config webpack.dev.js --open",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build:client": "PUBLIC_URL=/ webpack --config webpack.prod.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lifeanddoodles/react-boilerplate.git"
  },
  "keywords": [
    "webpack",
    "babel",
    "node",
    "react",
    "react-dom",
    "react-router-dom",
    "express"
  ],
  "author": "Sandra Vargas",
  "license": "ISC",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "fibers": "^4.0.2",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "dependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "express": "^4.17.1",
    "mongoose": "^5.8.10",
    "react": "^16.12.0",
    "react-dev-utils": "^9.1.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2"
  },
  "proxy": "http://localhost:5000"
}
