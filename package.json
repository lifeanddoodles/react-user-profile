{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Simple React boilerplate.",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "webpack-dev-server  --config webpack.dev.js --open",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build:client": "PUBLIC_URL=/ webpack --config webpack.prod.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lifeanddoodles/react-user-profile.git"
  },
  "keywords": [
    "webpack",
    "babel",
    "node",
    "react",
    "react-dom",
    "react-router-dom",
    "express"
  ],
  "author": "Sandra Vargas",
  "license": "ISC",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
