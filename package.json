    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.8.0",
    "fibers": "^4.0.3",
    "file-loader": "^5.1.0",
    "html-loader": "^0.5.5",
